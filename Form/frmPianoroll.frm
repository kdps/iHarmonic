Private Sub Form_Load()
Dim i As Long
Me.BackColor = RGB(61, 77, 87)
For i = 0 To pKey.Count - 1
    If pKey(i).Tag = "" Then
        pKey(i).Picture = Image2.Picture
    Else
        pKey(i).Picture = Image1.Picture
    End If
Next i
End Sub

Private Sub pKey_MouseDown(Index As Integer, Button As Integer, Shift As Integer, x As Single, Y As Single)

On Error Resume Next

pKeyon Index

PlayNote Index + 1

End Sub

Public Sub pKey_MouseUp(Index As Integer, Button As Integer, Shift As Integer, x As Single, Y As Single)
On Error Resume Next

StopNote Index + 1

pKeyoff Index

End Sub

Public Sub pKeyoff(Index As Integer)

On Error Resume Next

If pKey(Index).Tag = "" Then
    pKey(Index).Picture = Image2.Picture
    Line (pKey(Index).Width, pKey(Index).Top + 60)-(Me.ScaleWidth, (pKey(Index).Top + 180)), RGB(61, 77, 87), BF
Else
    Line (pKey(Index).Width, pKey(Index).Top + 60)-(Me.ScaleWidth, (pKey(Index).Top + 180)), RGB(61, 77, 87), BF
    pKey(Index).Picture = Image1.Picture
End If
End Sub

Public Sub pKeyon(Index As Integer)

On Error Resume Next

If pKey(Index).Tag = "" Then
    Line (pKey(Index).Width, pKey(Index).Top + 60)-(Me.ScaleWidth, (pKey(Index).Top + 180)), RGB(61, 81, 91), BF
    pKey(Index).Picture = Image6.Picture
Else
    Line (pKey(Index).Width, pKey(Index).Top + 60)-(Me.ScaleWidth, (pKey(Index).Top + 180)), RGB(61, 81, 91), BF
    pKey(Index).Picture = Image5.Picture
End If

End Sub

Private Sub pKey_KeyDown(Index As Integer, KeyCode As Integer, Shift As Integer)

Dim Note As Long

Note = KeyMap(KeyCode)
If Len(Note) Then
    If Not Note = lNote And Note Then
        PlayNote Note
    End If
End If

End Sub

Private Sub pKey_KeyUp(Index As Integer, KeyCode As Integer, Shift As Integer)

Dim Note As Long

Note = KeyMap(KeyCode)
If Len(Note) Then
    StopNote (Note)
End If

End Sub
