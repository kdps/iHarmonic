Public Sub cbType_Click()

On Error Resume Next

Dim NoteName() As String

txtTention = ""
lstTention.Clear
cbInv.Clear

'Show Tention Chart
Select Case cbType.Text
    Case "Maj" 'Major
        lstTention.AddItem "9"
    Case "min" 'Minor
        lstTention.AddItem "9"
    Case "M7" 'Major 7
        lstTention.AddItem "9"
        lstTention.AddItem "#11"
        lstTention.AddItem "13"
    Case "m7" 'Minor 7
        lstTention.AddItem "9"
        lstTention.AddItem "11"
    Case "Dim7sus4" '7sus4
        lstTention.AddItem "b9"
        lstTention.AddItem "#9"
        lstTention.AddItem "13"
    Case "Dim7" '7sus4
        lstTention.AddItem "b9"
        lstTention.AddItem "#9"
        lstTention.AddItem "#11"
        lstTention.AddItem "13"
        lstTention.AddItem "b13"
    Case "mM7" 'mM7
        lstTention.AddItem "7"
        lstTention.AddItem "13"
        lstTention.AddItem "b13"
    Case "m7b5" 'm7b5
        lstTention.AddItem "b13"
    Case "7" '7
        lstTention.AddItem "b9"
        lstTention.AddItem "#9"
        lstTention.AddItem "#11"
        lstTention.AddItem "13"
        lstTention.AddItem "b13"
    Case "M6" '6
        lstTention.AddItem "7"
        lstTention.AddItem "#11"
    Case "m6" 'm6
        lstTention.AddItem "7"
    Case "+7" '+7
        lstTention.AddItem "9"
        lstTention.AddItem "#11"
End Select

NoteName() = Split(AllNotePos(TexttoNote(cbRoot.Text), cbType.Text), ",") 'Split Note

For i = 0 To UBound(NoteName)
    cbInv.AddItem NotetoText(NoteName(i))
Next i

For i = 0 To 40
    If pKey(i).Tag = "1" Then
        pKey(i).BackColor = vbWhite
    Else
        pKey(i).BackColor = vbBlack
    End If
Next i

For i = 0 To UBound(NoteName)
    pKey(NoteName(i) - 1).BackColor = vbRed
Next i

If cbType.Text <> "" Then
    lstTention.Enabled = True
    cbInv.Enabled = True
    cmdPreview.Enabled = False
    cmdApply.Enabled = False
Else
    lstTention.Enabled = False
End If

cbInv.ListIndex = 0

End Sub

